slim 1 appearance zhangdb {
    instance dso hairRibgen $SLIMMASTER {
    	description {A RIB Generator that emits RIB for hair data.
	Attach to imported hair cache node.}
    subtype ribgen
	
	parameter string imagename {
		label {Image Name}
		default {ImageFile_File}
	}
	
	parameter string imagefile1 {
		label {Image File1}
	    subtype texture
	}
	
	parameter float maxframe1 {
		label {File1 Max Frame}
	    default 24
	}
	
	parameter string imagefile2 {
		label {Image File2}
	    subtype texture
	}
	
	parameter float maxframe2 {
		label {File2 Max Frame}
	    default 24
	}
	
	parameter string imagefile3 {
		label {Image File3}
	    subtype texture
	}
	
	parameter float maxframe3 {
		label {File3 Max Frame}
	    default 24
	}
	
	parameter string imagefile4 {
		label {Image File4}
	    subtype texture
	}
	
	parameter float maxframe4 {
		label {File4 Max Frame}
	    default 24
	}
	
	parameter string imagefile5 {
		label {Image File5}
	    subtype texture
	}
	
	parameter float maxframe5 {
		label {File5 Max Frame}
	    default 24
	}
  }
}
